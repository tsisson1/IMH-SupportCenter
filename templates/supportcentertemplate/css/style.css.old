/* Articles */

	/* code_block */
	.code_block
	{
		padding:2px;
		background:#F2F2F2;
		font-family: "Courier New", Courier, monospace;
		font-size:12px;
		border:1px solid #696969;
		color:#000;
		max-width:575px;
		white-space:nowrap;
		overflow:auto;
	}
	pre.code_block
	{
		white-space:pre;
	}
	pre:hover{cursor:auto;}

/* #question_details h1, */
.item-page .page-header {margin:0px 0px 10px 0px; border-bottom:1px solid #ddd;}
.item-page .page-header h1 {margin-top:0px; font-size:24px;}
.item-page .white_box h1 {margin-top:0px;}

/* article */
.item-page h1 {font-size:22px; color:#000; margin-top:30px;}
.item-page h2 {font-size:22px; color:#c52228; margin-top:30px;}
.item-page h3 {font-size:20px; color:#000; margin-top:30px;}
.item-page h4 {font-size:18px; color:#000; margin-top:30px;}
.item-page h5 {font-size:16px; color:#000; margin-top:30px;}
.item-page h6 {font-size:14px; color:#000; margin-top:30px;}

/* Your opinion matters */

#yom .author_image{width:60px; float:left; margin:0px 15px 15px 0px; border:1px solid #eee; }
.yom_intro{clear:both;}
#yom_success {color:red;}

/* Have a question */
.browse_other_questions{text-align:right;}

/* article_list */

ol.article_list { counter-reset: toc 0; padding:0px 0px 0px 50px; margin:0px; border:1px solid #ccc; background:#F8F8F8;}
ol.article_list li:before { content: "Step " counter(toc, decimal) "."; counter-increment: toc; font-weight:bold; float:left; margin:0px 0px 0px -55px; padding:0px; width:50px; }
ol.article_list li { list-style: none outside; padding: 5px 10px 5px 10px;}
ol.article_list .article_table td {background:#fff;}

ul.article_list li { padding:7px; }

.article_list img {margin-top:15px;}
.article_list .article_table {margin-top:15px;}
.article_list li h2, .article_list li h3, .article_list li h4, .article_list li h5, .article_list li h6 {margin-top:0px; padding-top:0px;}
.article_list li pre {max-width:595px;}
.article_list p.code_block {max-width:500px;}


ol.article_list_number li{list-style-type: decimal;}
ol.article_list_number li:before{content: none;font-weight: bold;}

/* article_table */

table.article_table { border-collapse: collapse; margin-top:5px; }
table.article_table th { border-width: 1px; padding: 4px; border-style: inset; border-color: gray; font-weight:bold; text-align:left; background:#ddd; vertical-align: top; }
table.article_table td { border-width: 1px; padding: 4px; border-style: inset; border-color: gray; background:#eee; }
table.article_table td .code_block {max-width:435px;}

