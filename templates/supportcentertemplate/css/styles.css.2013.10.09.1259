  -webkit-box-shadow: 0 3px 0 #666;
  box-shadow: 0 3px #666;
  color: #fff;
  margin-top: -2px;
  padding: 2px 40px;
  text-transform: uppercase;
}

.btn-longtail:hover,
.btn-longtail:active,
.btn-longtail.active,
.btn-longtail.disabled,
.btn-longtail[disabled] {
  background-color: #777;
  *background-color: #777;
  -moz-box-shadow: 0 3px 0 #666;
  -webkit-box-shadow: 0 3px #666;
  box-shadow: 0 3px #666;
  color: #fff;
}

.btn-longtail:active,
.btn-longtail.active {
  -moz-box-shadow: 0 3px 0 #666;
  -webkit-box-shadow: 0 3px 0 #666;
  box-shadow: 0 2px #666;
